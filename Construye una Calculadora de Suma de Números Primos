function esPrimo(number) {
  if (number <= 1) return false;

  for (let i = 2; i <= number**0.5; i++) {
    if (number % i === 0) {
      return false; 
    }
  }

  return true;
}

function sumPrimes(number) {
  let sumaFinal = 0
  for (let i = 0; i <= number; i++) {
    if (esPrimo(i)) {
      sumaFinal += i
    }
  }
  return sumaFinal
}
