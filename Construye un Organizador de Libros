const books = [
  {title: "titulo1", authorName: "nombre1", releaseYear: 1951},
  {title: "titulo2", authorName: "nombre2", releaseYear: 2162},
  {title: "titulo3", authorName: "nombre3", releaseYear: 3}
]


function sortByYear(book1, book2) {
  if (book1.releaseYear < book2.releaseYear){
    return -1
  }
  else if (book1.releaseYear > book2.releaseYear){
    return 1
  }
  return 0
}

const filteredBooks = books.filter(grado => grado.releaseYear < 1950)
filteredBooks.sort(sortByYear);
